{
}
rule actions =
    (-?)[0-9]+"."[0-9]*               { FLOAT }
    | "-"[0-9]+                       { SIGNED }
    | [0-9]+                          { INT }
    | "while"                         { WHILE }
    | [a-z]+                          { ID }
    | "\{"                            { L_BRACE }
    | "\}"                            { R_BRACE }
    | "="                             { ASSIGN }
    | ">"                             { GT }
    | ">="                            { GTE }
    | "<"                             { LT }
    | "<="                            { LTE }
    | "~"                               { CRACK_CARAJO }
    | (" "|"\n"|"\t"|"\s")+           { DELIM }
    | "return"           { RETURN }